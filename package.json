    "verify:icon": "node -e \"require('fs').accessSync('dist/nodes/Plunet/plunet.png');console.log('âœ… icon ok')\"",
    "prepublishOnly": "npm run build"
  },
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/PlunetApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/Plunet/Plunet.node.js"
    ]
  },
  "devDependencies": {
    "copyfiles": "^2.4.1",
    "n8n-workflow": "^1.82.0",
